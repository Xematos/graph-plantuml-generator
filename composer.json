{
    "name": "bartlett/graph-plantuml-generator",
    "description": "A PlantUML generator for graph-uml",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Laurent Laville",
            "email": "laurent.laville@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "jawira/plantuml-encoding": "^1.0"
    },
    "require-dev": {
        "jawira/plantuml": "^1.28"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/llaville/graphviz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bartlett\\GraphPlantUml\\": "src/"
        }
    }
}
